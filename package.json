{
  "name": "nano-query-2",
  "version": "1.0.5",
  "description": "Nano Query is a minimalistic and type strict library for creating custom query and mutate hooks in React. It leverages the power of nanostores, axios, and zod to provide a simple and efficient way to manage API requests and state in your application.",
  "author": "Mohammad Hosein Alijany",
  "keywords": [
    "react-query",
    "mutation",
    "query"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alijany/nano-query-2.git"
  },
  "license": "ISC",
  "source": "./script.ts",
  "type": "module",
  "exports": {
    "require": "./dist/nano-query-2.cjs",
    "default": "./dist/nano-query-2.modern.js"
  },
  "main": "./dist/nano-query-2.cjs",
  "module": "./dist/nano-query-2.module.js",
  "unpkg": "./dist/nano-query-2.umd.js",
  "types": "./dist/script.d.ts",
  "typings": "./dist/script.d.ts",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "axios": "^1.6.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "nanostores": "^0.9.5",
    "react": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "microbundle": "^0.15.1"
  }
}